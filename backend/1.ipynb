     "text": [
      "def upload_bytes(bucket: str, path: str, data: bytes, content_type: str = \"application/pdf\"):\n",
      "    \"\"\"\n",
      "    Upload raw bytes correctly to Supabase Storage.\n",
      "    Must use the `file=` kwarg, NOT BytesIO.\n",
      "    Must provide file_options.\n",
      "    \"\"\"\n",
      "    return supabase.storage.from_(bucket).upload(\n",
      "        path,\n",
      "        file=data,   # <---- RAW BYTES ONLY\n",
      "        file_options={\n",
      "            \"content-type\": content_type,\n",
      "            \"upsert\": True\n",
      "        }\n",
      "    )\n",
      "\n"
     ]
    }
   ],
   "source": [
    "from app.storage_service import upload_bytes\n",
    "import inspect\n",
    "print(inspect.getsource(upload_bytes))\n"
   ]
